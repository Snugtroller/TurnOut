{
  "expo": {
    "name": "turnout",
    "slug": "turnout",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/turnout.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash_turnout.png",
      "resizeMode": "contain",
      "backgroundColor": "#151718"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "entitlements": {
        "com.apple.developer.networking.wifi-info": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/turnout_mini.png",
        "backgroundColor": "#151718"
      }
    },
    "web": {
      "favicon": "./assets/turnout.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ]
    ]
  }
}
